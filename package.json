{
  "name": "@11ty/eleventy-img",
  "version": "4.0.2",
  "description": "Low level utility to perform build-time image transformations.",
  "publishConfig": {
    "access": "public"
  },
  "main": "img.js",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "pretest": "eslint img.js test/**.js",
    "test": "ava --no-worker-threads",
    "sample": "cd sample && node sample.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/11ty/eleventy-img.git"
  },
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/11ty"
  },
  "keywords": [
    "eleventy",
    "eleventy-utility"
  ],
  "author": {
    "name": "Zach Leatherman",
    "email": "zachleatherman@gmail.com",
    "url": "https://zachleat.com/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/11ty/eleventy-img/issues"
  },
  "homepage": "https://github.com/11ty/eleventy-img#readme",
  "dependencies": {
    "@11ty/eleventy-fetch": "^4.0.0",
    "brotli-size": "^4.0.0",
    "debug": "^4.3.4",
    "entities": "^4.5.0",
    "image-size": "^1.1.1",
    "p-queue": "^6.6.2",
    "sharp": "^0.33.2"
  },
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-plugin-webc": "^0.11.1",
    "ava": "^6.1.1",
    "eslint": "^8.56.0",
    "pixelmatch": "^5.3.0"
  },
  "ava": {
    "failFast": false,
    "files": [
      "./test/*.js"
    ],
    "watchMode": {
      "ignoreChanges": [
        "./.cache/*",
        "./img/*",
        "./test/img/*",
        "./test/**/generated*"
      ]
    }
  }
}
