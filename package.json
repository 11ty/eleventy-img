{
  "name": "@11ty/eleventy-img",
  "version": "0.6.0",
  "description": "Low level utility to perform build-time image transformations.",
  "publishConfig": {
    "access": "public"
  },
  "main": "img.js",
  "scripts": {
    "test": "eslint img.js test/**.js && ava",
    "sample": "cd sample && node sample.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/11ty/eleventy-img.git"
  },
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/11ty"
  },
  "keywords": [
    "eleventy",
    "eleventy-utility"
  ],
  "author": {
    "name": "Zach Leatherman",
    "email": "zachleatherman@gmail.com",
    "url": "https://zachleat.com/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/11ty/eleventy-img/issues"
  },
  "homepage": "https://github.com/11ty/eleventy-img#readme",
  "peerDependencies": {
    "@11ty/eleventy": ">=0.10.0"
  },
  "dependencies": {
    "@11ty/eleventy-cache-assets": "^2.0.3",
    "@saschazar/wasm-avif": "^1.0.0",
    "debug": "^4.3.1",
    "fs-extra": "^9.0.1",
    "image-size": "^0.9.3",
    "p-queue": "^6.6.2",
    "sharp": "^0.27.0",
    "short-hash": "^1.0.0"
  },
  "devDependencies": {
    "ava": "^3.14.0",
    "eslint": "^7.16.0"
  },
  "ava": {
    "failFast": false,
    "files": [
      "./test/*.js"
    ],
    "ignoredByWatcher": [
      "./test/img/*"
    ]
  }
